<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App.HaBHAApiConsumer.Pages.EditPage"
             Title="EditPage">

    <VerticalStackLayout>
        <Entry x:Name="txtDescriptions" Placeholder="Descriptions"/>
        <Entry x:Name="txtLocations" Placeholder="Locations"/>
        <Entry x:Name="txtRoomsize" Placeholder="Roomsize"/>

        <Button x:Name="btnSave" Text="save" Margin="0, 20" Clicked="btnSave_Clicked"/>
        <Button x:Name="btnCancel" Text="save" Margin="0, 20" Clicked="btnCancel_Clicked"/>
    </VerticalStackLayout>
</ContentPage>